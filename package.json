{
    "name": "sportlab-next",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
        "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
        "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
        "payload:generate": "pnpm run generate:db && pnpm run generate:importmap && pnpm run generate:types && pnpm run generate:types:cloudflare",
        "generate:db": "cross-env NODE_OPTIONS=--no-deprecation payload generate:db-schema",
        "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
        "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
        "generate:types:cloudflare": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
        "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
        "migrate:fresh": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:fresh",
        "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
        "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
        "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
        "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
        "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview --env=$CLOUDFLARE_ENV",
        "deploy": "pnpm run deploy:database && pnpm run deploy:app",
        "deploy:version": "pnpm run deploy:database && pnpm run deploy:upload",
        "deploy:upload": "opennextjs-cloudflare build --env=$CLOUDFLARE_ENV && opennextjs-cloudflare upload --env=$CLOUDFLARE_ENV",
        "deploy:app": "opennextjs-cloudflare build --env=$CLOUDFLARE_ENV && opennextjs-cloudflare deploy --env=$CLOUDFLARE_ENV",
        "deploy:database": "cross-env NODE_ENV=production PAYLOAD_SECRET=ignore payload migrate && wrangler d1 execute D1 --command 'PRAGMA optimize' --env=$CLOUDFLARE_ENV --remote"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.940.0",
        "@hookform/resolvers": "^5.1.1",
        "@next/third-parties": "^16.0.3",
        "@opennextjs/cloudflare": "^1.13.1",
        "@payloadcms/admin-bar": "^3.68.5",
        "@payloadcms/db-d1-sqlite": "^3.68.5",
        "@payloadcms/email-nodemailer": "^3.68.5",
        "@payloadcms/live-preview-react": "^3.68.5",
        "@payloadcms/next": "^3.68.5",
        "@payloadcms/plugin-cloud-storage": "^3.68.5",
        "@payloadcms/plugin-form-builder": "^3.68.5",
        "@payloadcms/plugin-nested-docs": "^3.68.5",
        "@payloadcms/plugin-redirects": "^3.68.5",
        "@payloadcms/plugin-seo": "^3.68.5",
        "@payloadcms/richtext-lexical": "^3.68.5",
        "@payloadcms/storage-r2": "^3.68.5",
        "@payloadcms/storage-s3": "^3.68.5",
        "@payloadcms/translations": "^3.68.5",
        "@payloadcms/ui": "^3.68.5",
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-label": "^2.1.8",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slot": "^1.2.4",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cross-env": "^10.1.0",
        "embla-carousel-react": "^8.6.0",
        "graphql": "^16.12.0",
        "graphql-tag": "^2.12.6",
        "he": "^1.2.0",
        "lucide-react": "^0.554.0",
        "next": "^15.5.9",
        "next-recaptcha-v3": "^1.5.2",
        "next-sitemap": "^4.2.3",
        "next-turnstile": "^1.0.7",
        "payload": "^3.68.5",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "react-hook-form": "^7.60.0",
        "striptags": "^3.2.0",
        "tailwind-merge": "^3.4.0",
        "zod": "^3.25.74"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@tailwindcss/typography": "^0.5.19",
        "@types/he": "^1.2.3",
        "@types/node": "^20",
        "@types/react": "^19.2.3",
        "@types/react-dom": "^19.2.3",
        "autoprefixer": "^10.4.21",
        "eslint": "^9",
        "eslint-config-next": "15.3.1",
        "postcss": "^8.5.3",
        "serve": "^14.2.5",
        "tailwindcss": "^4.1.4",
        "typescript": "^5",
        "wrangler": "^4.49.1"
    }
}
