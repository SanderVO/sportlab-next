{
    "name": "sportlab-next",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
        "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
        "db:import": "wrangler d1 execute D1 --local --file=data.sql",
        "db:disable-foreign-keys": "sh -c '{ echo \"PRAGMA foreign_keys=OFF;\"; echo \"PRAGMA defer_foreign_keys=TRUE;\"; grep -E \"^INSERT INTO\" prod.sql; echo \"PRAGMA foreign_keys=ON;\"; } > data.sql'",
        "db:export": "rm -rf .wrangler/state/v3/d1 && wrangler d1 export D1 --remote --env production --output=prod.sql && grep -E '^INSERT INTO' prod.sql > data.sql",
        "db:sync-prod": "pnpm run db:export && pnpm run migrate && pnpm run db:disable-foreign-keys && pnpm run db:import",
        "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
        "payload:generate": "pnpm run generate:types && pnpm run generate:types:cloudflare && pnpm run generate:db && pnpm run generate:importmap",
        "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
        "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
        "generate:types:cloudflare": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
        "generate:db": "cross-env NODE_OPTIONS=--no-deprecation payload generate:db-schema",
        "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
        "migrate:fresh": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:fresh",
        "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
        "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
        "preview": "cross-env NODE_OPTIONS=--no-deprecation CLOUDFLARE_ENV=preview opennextjs-cloudflare build && opennextjs-cloudflare preview --env=preview",
        "deploy": "pnpm run deploy:database:production && pnpm run deploy:build && pnpm run deploy:app",
        "deploy:version": "pnpm run deploy:database:production && pnpm run deploy:build && pnpm run deploy:upload",
        "deploy:build": "CLOUDFLARE_ENV=production opennextjs-cloudflare build --env=production",
        "deploy:upload": "CLOUDFLARE_ENV=production opennextjs-cloudflare upload --env=production",
        "deploy:app": "CLOUDFLARE_ENV=production opennextjs-cloudflare deploy --env=production",
        "deploy:database:preview": "cross-env NODE_ENV=production CLOUDFLARE_ENV=preview PAYLOAD_SECRET=ignore payload migrate && wrangler d1 execute D1 --command 'PRAGMA optimize' --env=preview --remote",
        "deploy:database:production": "cross-env NODE_ENV=production CLOUDFLARE_ENV=production PAYLOAD_SECRET=ignore payload migrate && wrangler d1 execute D1 --command 'PRAGMA optimize' --env=$CLOUDFLARE_ENV --remote"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.940.0",
        "@hookform/resolvers": "^5.1.1",
        "@next/third-parties": "^15.5.9",
        "@opennextjs/cloudflare": "^1.14.10",
        "@payloadcms/admin-bar": "^3.69.0",
        "@payloadcms/db-d1-sqlite": "^3.69.0",
        "@payloadcms/email-nodemailer": "^3.69.0",
        "@payloadcms/live-preview-react": "^3.69.0",
        "@payloadcms/next": "^3.69.0",
        "@payloadcms/plugin-cloud-storage": "^3.69.0",
        "@payloadcms/plugin-form-builder": "^3.69.0",
        "@payloadcms/plugin-nested-docs": "^3.69.0",
        "@payloadcms/plugin-redirects": "^3.69.0",
        "@payloadcms/plugin-seo": "^3.69.0",
        "@payloadcms/richtext-lexical": "^3.69.0",
        "@payloadcms/storage-r2": "^3.69.0",
        "@payloadcms/storage-s3": "^3.69.0",
        "@payloadcms/translations": "^3.69.0",
        "@payloadcms/ui": "^3.69.0",
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-label": "^2.1.8",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slot": "^1.2.4",
        "ahooks": "^3.9.6",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cross-env": "^10.1.0",
        "embla-carousel-react": "^8.6.0",
        "he": "^1.2.0",
        "lucide-react": "^0.554.0",
        "next": "^15.5.9",
        "next-sitemap": "^4.2.3",
        "next-turnstile": "^1.0.7",
        "payload": "^3.69.0",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "react-hook-form": "^7.60.0",
        "tailwind-merge": "^3.4.0",
        "zod": "^3.25.74"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@next/eslint-plugin-next": "^15.5.9",
        "@tailwindcss/postcss": "^4",
        "@tailwindcss/typography": "^0.5.19",
        "@types/he": "^1.2.3",
        "@types/node": "^20",
        "@types/react": "^19.2.3",
        "@types/react-dom": "^19.2.3",
        "autoprefixer": "^10.4.21",
        "baseline-browser-mapping": "^2.9.14",
        "eslint": "^9",
        "eslint-config-next": "15.5.9",
        "eslint-plugin-react-hooks": "^7.0.1",
        "postcss": "^8.5.3",
        "serve": "^14.2.5",
        "tailwindcss": "^4.1.4",
        "typescript": "^5",
        "wrangler": "^4.59.3"
    }
}
